.portfolio-modal .close-modal .lr {
  height: 40px;
  width: 6px;
  margin-left: 35px;
  background-color: #ffffff;
  transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -webkit-border-radius: 20%;
  -moz-border-radius: 20%;
  -ms-border-radius: 20%;
  border-radius: 20%;
  /* IE 9 */
  -webkit-transform: rotate(45deg);
  /* Safari and Chrome */
  z-index: 1051;
  -webkit-box-shadow: 0px 1px 5px 1px rgba(0, 0, 0, .2) inset;
  -moz-box-shadow: 0px 1px 5px 1px rgba(0, 0, 0, .2) inset;
  -ms-box-shadow: 0px 1px 5px 1px rgba(0, 0, 0, .2) inset;
  -o-box-shadow: 0px 1px 5px 1px rgba(0, 0, 0, .2) inset;
  box-shadow: 0px 1px 5px 1px rgba(0, 0, 0, .2) inset;
}
