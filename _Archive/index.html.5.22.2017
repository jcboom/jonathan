 <!-- Filter Functionality -->
 <script>
 /*******************************************************************************
 * Author: Jonathan Calzada
 * Purpose: This handles the filter and switch functionality
 * Date:
 *******************************************************************************/
 function toggleFilter(filter, color) {
	 var strCards=".card-"+filter.substring(filter.lastIndexOf("-")+1,filter.lastIndexOf(""));
		if( $("#"+filter).prop('className').indexOf(filter+"-deco") != -1) {
 			$("."+filter+"-button").html("&plus;");
 			$("."+filter+"-button").css("color", "#AAAAAA");
			$(strCards).hide();
		} else {
 			$("."+filter+"-button").html("&times;");
 			$("."+filter+"-button").css("color", color);
			$("#cards").show(); //Displays cards section
			$(strCards).show(); //Displays card types
 		}
 	  $("#"+filter).toggleClass(filter+'-deco');
 		$("#"+filter).toggleClass('cards-filter-item-disabled');

		/* SWITCH MECHANISM OFF/ON POSITION */
		var enableSwitch = true;
		$('div','#main').each(function(){
			if( $(this).prop('className').indexOf("disabled") != -1) {
				enableSwitch = false;
			}
    });
		if (enableSwitch == false) {
			$('#slider-mech').prop('checked', false);
		} else {
			enableSwitch = true;
			$('#slider-mech').prop('checked', true);
		}
 }

$(document).ready(function() {

	/* Need to simplify the function below and create veriables so its not DRY (Don't Repeat Yourself) */
	$("#slider-mech").click(function() {
		$('#cards').children('div').each(function () {
			var strClass = $(this).prop("className")
			var strCardType=strClass.substring(strClass.lastIndexOf("card-")+5,strClass.lastIndexOf("card-")+7);
			var strCard=strClass.substring(strClass.lastIndexOf("card-"),strClass.lastIndexOf("card-")+7);
			//toggleFilter("filter-"+strCard,"red");
			$("."+strCard).show();
		});

		if( $("#slider-mech").prop("checked") ) { //When swtich is turned on
			//$("#cards").show();

			$(".closebtn").html("&times;"); //Changes + to x sign on buttons

			$("#filter-an").removeClass('cards-filter-item-disabled');
			$("#filter-an").addClass('filter-an-deco');
			$(".filter-an-button").css("color", "#8B7C6D");

			$("#filter-ux").removeClass('cards-filter-item-disabled');
			$("#filter-ux").addClass('filter-ux-deco');
			$(".filter-ux-button").css("color", "#5A0AA1");

			$("#filter-ph").removeClass('cards-filter-item-disabled');
			$("#filter-ph").addClass('filter-ph-deco');
			$(".filter-ph-button").css("color", "#0197B2");

			$("#filter-hx").removeClass('cards-filter-item-disabled');
			$("#filter-hx").addClass('filter-hx-deco');
			$(".filter-hx-button").css("color", "#272727");
		} else { //When swtich is turned off
			//$("#cards").hide();

			// Checks card container for cards and disables want is found
			$('#cards').children('div').each(function () {
				var strClass = $(this).prop("className")
				var strCardType=strClass.substring(strClass.lastIndexOf("card-")+5,strClass.lastIndexOf("card-")+7);
				var strCard=strClass.substring(strClass.lastIndexOf("card-"),strClass.lastIndexOf("card-")+7);
				//toggleFilter("filter-"+strCard,"red");
				$("."+strCard).hide();
			});
			/* alert($("#cards").children().length); //Total Number of cards */
			$(".closebtn").css("color", "#AAAAAA"); //Makes all close buttons gray
			$(".closebtn").html("&plus;"); //Changes all buttons to Plus signs



			$("#filter-an").removeClass('filter-an-deco');
			$("#filter-an").addClass('cards-filter-item-disabled');

			$("#filter-ux").removeClass('filter-ux-deco');
			$("#filter-ux").addClass('cards-filter-item-disabled');

			$("#filter-ph").removeClass('filter-ph-deco');
			$("#filter-ph").addClass('cards-filter-item-disabled');

			$("#filter-hx").removeClass('filter-hx-deco');
			$("#filter-hx").addClass('cards-filter-item-disabled');

		}
	});
});

 </script>
